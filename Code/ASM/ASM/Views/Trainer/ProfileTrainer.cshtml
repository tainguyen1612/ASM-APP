@model ASM.Models.Profile_User
@{
                /**/

                ViewBag.Title = "ProfileTrainer";
                Layout = "~/Views/Shared/_LayoutPage3.cshtml";
}

<div id="content-header">
    <div id="breadcrumb"> <a href="#" title="Go to Home" class="tip-bottom"><i class="icon-home"></i> Home</a> <a href="#" class="current">Profile</a> </div>
    <h1>Profile</h1>
</div>
<div class="container-fluid">
    <hr>
    <div class="row-fluid">
        <div class="span12">
            <div class="widget-box">
                <div class="widget-content nopadding">

                        <div class="control-group">
                            <label class="control-label">UserID</label>
                            <div class="controls" style="font-size : 20px; font-weight : bold">
                                @Html.DisplayFor(x => x.UserID)
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Full Name</label>
                            <div class="controls">
                                @Html.DisplayFor(x => x.Full_Name)
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Address</label>
                            <div class="controls">
                                @Html.DisplayFor(x => x.Address)
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Phone</label>
                            <div class="controls">
                                @Html.DisplayFor(x => x.Phone)
                            </div>
                        </div>
                        <div class="control-group">
                            <label class="control-label">Date of Birth</label>
                            <div class="controls">
                                @Html.DisplayFor(x => x.DateOfBirth)
                            </div>
                        </div>
                        <div class="form-actions">
                            <button type="submit" class="btn btn-success">Edit</button>
                        </div>

                </div>
            </div>
        </div>
    </div>
</div>
